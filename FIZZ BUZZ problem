#include <stdio.h>

int main() {
    int i,j,arr[100];
    for(i=1;i<=100;i++)
    {
        if(i%3==0 && i%5==0)
       {
           printf("\n%d is FIZZBUZZ",i);
       }
        
       else{
            if(i%3==0)
            {
            printf("\n%d is FIZZ",i);
            }
            else if(i%5==0)
            {
            printf("\n%d is BUZZ",i);
            }
            else
            {
                printf("\n%d ",i);
            }
           }
    }

    return 0;
}

